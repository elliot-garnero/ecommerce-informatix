<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Users" table="users">
    <unique-constraints>
      <unique-constraint name="UNIQ_1483A5E9E7927C74" columns="email"/>
      <unique-constraint name="UNIQ_1483A5E9F85E0677" columns="username"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roles" type="text" column="roles" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countries" type="string" column="countries" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cp" type="integer" column="cp" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="deleted" type="boolean" column="deleted" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
  </entity>
</doctrine-mapping>
