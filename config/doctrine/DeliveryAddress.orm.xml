<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\DeliveryAddress" table="delivery_address">
    <id name="idDeliv" type="integer" column="id_deliv">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="delLastname" type="string" column="del_lastname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="delFirstname" type="string" column="del_firstname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="delAddress" type="string" column="del_address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="delCp" type="integer" column="del_cp" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="delCity" type="string" column="del_city" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="delCountries" type="string" column="del_countries" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false"/>
  </entity>
</doctrine-mapping>
