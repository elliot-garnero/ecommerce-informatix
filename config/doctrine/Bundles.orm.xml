<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Bundles" table="bundles">
    <id name="idBundle" type="integer" column="id_bundle">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bundName" type="string" column="bund_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bundDescription" type="string" column="bund_description" length="1500" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="bundPicture1" type="string" column="bund_picture1" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="bundPicture2" type="string" column="bund_picture2" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="bundPicture3" type="string" column="bund_picture3" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="bundPicture4" type="string" column="bund_picture4" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="bundProducts" type="string" column="bund_products" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bundStock" type="integer" column="bund_stock" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bundPrice" type="integer" column="bund_price" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
